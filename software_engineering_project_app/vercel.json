{
  "version": 2,
  "public": false,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://eatalyze-production.up.railway.app/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}